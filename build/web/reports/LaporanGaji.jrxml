    <queryString>
        <![CDATA[SELECT g.kodeGaji, 
                        g.tanggal, 
                        k.nama AS namaKaryawan, 
                        p.namaPekerjaan, 
                        g.jumlahTugas, 
                        g.gajiPokok, 
                        g.bonus, 
                        g.totalGaji
                 FROM tbgaji g 
                 LEFT JOIN tbkaryawan k ON g.ktp = k.ktp 
                 LEFT JOIN tbpekerjaan p ON g.kodePekerjaan = p.kodePekerjaan 
                 ORDER BY g.tanggal DESC]]>
    </queryString>
    
